<h1>Listing attached_photos</h1>

<table>
  <tr>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
  </tr>

<% @attached_photos.each do |attached_photo| %>
  <tr>
    <td><%=h attached_photo.photo_file_name %></td>
    <td><%=h attached_photo.photo_content_type %></td>
    <td><%=h attached_photo.photo_file_size %></td>
    <td><%= link_to 'Show', attached_photo %></td>
    <td><%= link_to 'Edit', edit_attached_photo_path(attached_photo) %></td>
    <td><%= link_to 'Destroy', attached_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New attached_photo', new_attached_photo_path %>